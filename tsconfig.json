{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "experimentalDecorators": true
    },
    "include": [
        "src/ts/*.ts",
        "src/ts/*.vue",
        "test/*.ts",
    ],
    "exclude": [
        "node_modules"
    ],
}